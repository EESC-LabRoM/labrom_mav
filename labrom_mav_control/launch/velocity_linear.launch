<launch>
<node name="Controller" pkg="labrom_mav_control" type="velocity_linear_node" output="screen">
  <rosparam file="$(find labrom_mav_control)/params/vrep.yaml"/>
  <!-- Subscribers -->
  <remap from="/trajectory" to="/trajectory"/>
  <remap from="/odometry" to="/odometry/filtered"/>
  <!-- Publishers 
  <remap from="/controller/velocity/cmd_thrust" to="/cmd_thrust"/>
  <remap from="/controller/velocity/cmd_attitude" to="/cmd_attitude"/>
   -->
</node>
</launch>
